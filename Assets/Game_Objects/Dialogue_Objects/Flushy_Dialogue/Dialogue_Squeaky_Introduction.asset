  m_Script: {fileID: 11500000, guid: 974d03248160ac84dad927547d7629eb, type: 3}
  m_Name: Dialogue_Squeaky_Introduction
  m_EditorClassIdentifier: Assembly-CSharp::DialogueBranch
  Nodes:
  - DialogueText: <color=orange>What can I get you, Detective?</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>Get me a glass of moss-wine.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>You have a keen sight for a rat. What gave me away?</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>Was it my raspy voice, my somber look, my introspective
      demeanor?</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>It's the hat, Detective.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: "<color=yellow>I\u2019ll have you know I\u2019ve got a great deal
      on this hat.</color>"
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>...</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>And it suits my trench coat.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>Certainly...</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>I'm looking for a rat... a rat capable of anything.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>What makes a rat capable of anything? Is it desperation,
      Detective?</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>Are you looking for a <color=green>desperate rat</color>?</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: "<color=orange>My sister says despair is gambled in casino chips.</color>\r"
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: "<color=yellow>You\u2019re singing too much poetry for a bartender.
      Get to the point.</color>"
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>My bar attracts the anguished, the desperate, those
      looking to drown their sorrows in cheese.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: "<color=orange>I\u2019d advise talking to <color=white>Flushy at
      the entrance</color>.</color>"
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: "<color=yellow>What can you tell me about this <color=green>\u201CFlushy\u201D</color>?</color>"
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>Settle your whiskers, Detective. Information is not
      merely gifted.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=orange>Indulge me in a game and I will tell you all I know.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
  - DialogueText: <color=yellow>(I down my moss-wine in one swig and feel my fur
      spring up.)</color>
    DialogueSound: {fileID: 8300000, guid: 65f5d7d995d1a714eb3cdc5bf7ed0f1b, type: 3}
    Evidence: 
  - DialogueText: <color=yellow>I'll nibble, Bartender.</color>
    DialogueSound: {fileID: 0}
    Evidence: 
